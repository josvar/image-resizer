{
  "name": "@josvar/image-resizer",
  "version": "0.1.1",
  "description": "Image resizer",
  "main": "lib/cli.js",
  "bin": "lib/cli.js",
  "scripts": {
    "prebabelify": "rimraf lib",
    "babelify": "babel src --out-dir lib",
    "prepare": "npm run babelify",
    "style": "eslint .",
    "test": "jest && eslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/josvar/image-resizer.git"
  },
  "keywords": [
    "image",
    "resize",
    "cli"
  ],
  "author": "Josue Vargas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/josvar/image-resizer/issues"
  },
  "homepage": "https://github.com/josvar/image-resizer",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "jest": "^23.3.0",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "prettier": {
    "semi": false,
